{
    "manifest_version": 2,
    "name": "NEW TAB",
    "description": "A simple new tab page with a WYSIWYG editor embedded.",
    "version": "1.0",
    "permissions": [
          "storage"
        ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "options_ui": {
      "page": "options.html",
      "chrome_style": true
    },
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
 }
